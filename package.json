{
  "name": "edgeroutine",
  "version": "0.0.11",
  "description": "This is a component for alibaba cloud dcdn",
  "autoInstall": false,
  "homepage": "https://www.serverless-devs.com",
  "author": "Serverless-Devs",
  "contributors": [],
  "license": "MIT",
  "main": "./dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "Serverless",
    "Framework",
    "Serverless-Devs",
    "Serverless-Tool",
    "DCDN",
    "edgeroutine"
  ],
  "scripts": {
    "start": "npm run watch",
    "watch": "tsc -w",
    "prebuild": "rimraf package-lock.json && rimraf dist && rimraf node_modules && npm i",
    "build": "esbuild src/index.ts --bundle --log-level=error --minify --platform=node --format=cjs --target=node10.4 --external:@serverless-devs/core --outfile=dist/index.js"
  },
  "dependencies": {
    "@alicloud/dcdn20180115": "^1.0.24",
    "@alicloud/openapi-client": "^0.4.4",
    "@alicloud/tea-util": "^1.4.5",
    "@serverless-devs/core": "latest",
    "axios": "^1.2.0",
    "inquirer": "^8.2.5",
    "lodash.get": "^4.4.2"
  },
  "devDependencies": {
    "@types/node": "^14.0.23",
    "esbuild": "^0.13.8",
    "rimraf": "^3.0.2",
    "ts-node": "^8.10.2",
    "typescript": "^4.4.2"
  }
}
